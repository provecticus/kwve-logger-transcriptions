{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Asset",
  "type": "object",
  "required": [
    "id",
    "source",
    "station",
    "utc_start",
    "utc_end",
    "minio_object"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "station": {
      "type": "string"
    },
    "program": {
      "type": [
        "string",
        "null"
      ]
    },
    "utc_start": {
      "type": "string",
      "format": "date-time"
    },
    "utc_end": {
      "type": "string",
      "format": "date-time"
    },
    "duration_sec": {
      "type": "integer"
    },
    "sha256": {
      "type": "string"
    },
    "codec": {
      "type": [
        "string",
        "null"
      ]
    },
    "sample_rate": {
      "type": [
        "integer",
        "null"
      ]
    },
    "channels": {
      "type": [
        "integer",
        "null"
      ]
    },
    "minio_object": {
      "type": "string"
    },
    "retention_expiry_ts": {
      "type": [
        "string",
        "null"
      ],
      "format": "date-time"
    },
    "legal_hold": {
      "type": "boolean"
    }
  }
}